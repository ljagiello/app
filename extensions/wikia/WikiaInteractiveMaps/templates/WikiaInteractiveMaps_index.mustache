<table>
	<thead>
		<tr>
			<th>Map name</th>
			<th>Type</th>
			<th>Original image (only for custom)</th>
			<th>Status</th>
		</tr>
	</thead>
	<tbody>
	{{#maps}}
		<tr>
			<td><a href="{{url}}">{{name}}</a></td>
			<td>{{type}}</td>
			<td>
				{{#image}}
					<a href="{{image}}">link</a>
				{{/image}}
			</td>
			<td>{{status}}</td>
		</tr>
	{{/maps}}

	{{^maps}}
		<tr>
			<td colspan="3">There are no maps on this wiki yet.</td>
		</tr>
	{{/maps}}

	</tbody>
</table>
